programa {
  funcao inicio() {
    inteiro numero
    cadeia mensagem

    escreva("Digite um numero de 1 à 100: ")
    leia(numero)

    mensagem ="Os proximos numeros : " + (numero + 1) + ", " + (numero + 2) + " e " + (numero + 3) + "."

    escreva("O seu número é: " + numero + "\n")
    
    escreva("O dobro do seu número é: " + numero * 2 + "." + "\n")

    escreva(mensagem)


    

    

    
  }
}
